<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="704.0" prefWidth="974.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxproyectoguiado.controladores.FXMLAnteproyectosValidacionMenuController">
   <children>
      <Pane prefHeight="83.0" prefWidth="980.0" style="-fx-background-color: #516aab;">
         <children>
            <Label layoutX="118.0" layoutY="19.0" text="Administración de Validación de Anteproyectos" textFill="WHITE">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <ImageView fitHeight="70.0" fitWidth="75.0" layoutX="14.0" layoutY="7.0" nodeOrientation="RIGHT_TO_LEFT" onMouseClicked="#clicCerrarVentana" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../recursos/flecha-derecha.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <TableView fx:id="tvAnteproyectosValidados" layoutX="21.0" layoutY="145.0" prefHeight="480.0" prefWidth="935.0">
        <columns>
          <TableColumn fx:id="colNombreAnteproyecto" prefWidth="122.0" text="Nombre" />
            <TableColumn fx:id="colFechaInicio" prefWidth="144.0" text="Fecha de Inicio" />
          <TableColumn fx:id="colFechaFin" prefWidth="138.0" text="Fecha de Fin" />
            <TableColumn fx:id="colModalidad" prefWidth="130.0" text="Modalidad" />
            <TableColumn fx:id="colLGAC" minWidth="0.0" prefWidth="158.0" text="LGAC" />
            <TableColumn fx:id="colEstado" minWidth="7.0" prefWidth="132.0" text="Estado" />
            <TableColumn fx:id="colResponsable" prefWidth="110.0" text="Responsable" />
        </columns>
      </TableView>
      <Button layoutX="587.0" layoutY="647.0" mnemonicParsing="false" onAction="#clicBtnValidarAnteproyecto" text="Validar anteproyecto">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="785.0" layoutY="647.0" mnemonicParsing="false" onAction="#clicBtnDenegarAnteproyecto" text="Denegar anteproyecto">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="779.0" layoutY="96.0" mnemonicParsing="false" onAction="#clicBtnConsultarAnteproyecto" text="Consultar anteproyecto">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
